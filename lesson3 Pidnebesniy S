import random
from random import randint

#UNIT1
min=randint(0,59)
print(min)

if min >=0 and min< 15:
    print("число знаходиться в першій чверті")
if min >=16 and min< 30:
    print("число знаходиться в другій чверті")
if min >=31 and min< 45:
    print("число знаходиться в третій чверті")
if min >=45 and min< 60:
    print("число знаходиться в четвертій чверті")

#UNIT2
birth_month=int(input("Запишіть номер місяця вашого народження"))

if birth_month == 1:
    print("Зима - За вікном падав сніг")
elif birth_month == 12:
    print("Зима - За вікном падав сніг")
elif birth_month == 2:
    print("Зима - За вікном падав сніг")
elif birth_month == 3:
    print("Весна - Все довкола розцвітало")
elif birth_month == 4:
    print("Весна - Все довкола розцвітало")
elif birth_month == 5:
    print("Весна - Все довкола розцвітало")
elif birth_month == 6:
    print("Літо - Діти насолоджувались літніми канікулами")
elif birth_month == 7:
    print("Літо - Діти насолоджувались літніми канікулами")
elif birth_month == 8:
    print("Літо - Діти насолоджувались літніми канікулами")
elif birth_month == 9:
    print("Осінь - Все довкола загоралось яскравими фарбами")
elif birth_month == 10:
    print("Осінь - Все довкола загоралось яскравими фарбами")
elif birth_month == 11:
    print("Осінь - Все довкола загоралось яскравими фарбами")
